<svg xmlns="http://www.w3.org/2000/svg" width="180" height="180" viewBox="0 0 180 180">
  <!-- Background -->
  <rect width="180" height="180" fill="#2d1b3d" rx="40"/>

  <!-- Lighthouse tower icon (centered and scaled) -->
  <g transform="translate(65, 30) scale(3)">
    <!-- Tower base -->
    <rect x="4" y="20" width="8" height="4" fill="#f4f1f5"/>
    <!-- Tower body -->
    <polygon points="3,20 13,20 11,6 5,6" fill="#f4f1f5"/>
    <!-- Stripes -->
    <rect x="5" y="10" width="6" height="2" fill="#2d1b3d"/>
    <rect x="5" y="14" width="6" height="2" fill="#2d1b3d"/>
    <!-- Light beacon -->
    <path d="M 8,2 L 5,6 L 11,6 Z" fill="#fbbf24"/>
    <!-- Light rays -->
    <line x1="8" y1="2" x2="1" y2="0" stroke="#fbbf24" stroke-width="1.5" opacity="0.7"/>
    <line x1="8" y1="2" x2="15" y2="0" stroke="#fbbf24" stroke-width="1.5" opacity="0.7"/>
    <line x1="8" y1="3" x2="0" y2="5" stroke="#fbbf24" stroke-width="1" opacity="0.5"/>
    <line x1="8" y1="3" x2="16" y2="5" stroke="#fbbf24" stroke-width="1" opacity="0.5"/>
  </g>
</svg>
